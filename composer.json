{
    "name": "andrey-helldar/move-db",
    "description": "Easy data transfer from one database to another",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "database",
        "migrating",
        "migration",
        "move",
        "sync",
        "transfer"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/move-db/issues",
        "source": "https://github.com/andrey-helldar/move-db"
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\MoveDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
