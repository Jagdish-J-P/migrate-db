{
    "name": "dragon-code/migrate-db",
    "description": "Easy data transfer from one database to another",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "database",
        "migrating",
        "migration",
        "move",
        "transfer",
        "dragon-code",
        "dragon",
        "andrey-helldar"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/migrate-db/issues",
        "source": "https://github.com/TheDragonCode/migrate-db"
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-pdo": "*",
        "dragon-code/contracts": "^2.8",
        "dragon-code/support": "^5.0",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "ext-pdo_mysql": "*",
        "ext-pdo_pgsql": "*",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DragonCode\\MigrateDB\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "DragonCode\\MigrateDB\\ServiceProvider"
            ]
        }
    }
}
